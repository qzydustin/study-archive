/*
 * File: globalopt-06
 * Tests: dead code elimination involving loops
 *
 * All the assignments to y are dead
 */

extern void println(int x);

void main(void) {
    int x, y;

    for (x = 0; x < 1000; x = x + 1) {
        y = 0;
        y = 2*y + 1;
        y = 2*y + 1;
        y = 2*y + 1;
        y = 2*y + 1;
        y = 2*y + 1;
    }

    println(x);  
}

